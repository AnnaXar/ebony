{
    "name": "@ebenos/framework",
    "version": "4.0.0-alpha.3",
    "description": "A module-based NodeJS chatbot framework.",
    "main": "./build/index.js",
    "types": "./build/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "npx tsc",
        "prepare": "yarn run build",
        "test": "yarn run build && yarn node ./build/index.js",
        "prettier:base": "npx prettier --config ../../.prettierrc",
        "prettier:check": "yarn run prettier:base -- --list-different \"lib/**/*.{ts,tsx}\"",
        "prettier:write": "yarn run prettier:base -- --write \"lib/**/*.{ts,tsx}\"",
        "lint": "eslint '*/**/*.ts' --quiet --fix -c ../../.eslintrc.js --ignore-path ../../.eslintignore",
        "unit-test": "ts-mocha -p src/tsconfig.json src/tests/**/*.spec.ts"
    },
    "keywords": [
        "bot",
        "chatbot",
        "messenger-platform",
        "messenger",
        "facebook",
        "bot-framework",
        "framework",
        "ebony-framework",
        "ebony"
    ],
    "author": "Christos Panagiotakopoulos <chrispanag@gmail.com>",
    "repository": "git://github.com/chrispanag/ebony.git",
    "license": "MIT",
    "dependencies": {
        "@types/lodash": "^4.14.165",
        "@types/mongodb": "^3.6.3",
        "lodash": "^4.17.20",
        "mongodb": "^3.6.3",
        "mongoose": "^5.11.8"
    },
    "engines": {
        "node": ">=8.9.0"
    },
    "files": [
        "LICENSE",
        "README.md",
        "build/**/*"
    ],
    "gitHead": "60576b5ea6994bbd98a9b914c4b2e9b4c511ce24"
}
